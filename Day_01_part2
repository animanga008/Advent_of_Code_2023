str2num = {
    "one": "o1e",
    "two": "t2o",
    "three": "t3e",
    "four": "f4r",
    "five": "f5e",
    "six": "s6x",
    "seven": "s7n",
    "eight": "e8t",
    "nine": "n9e",
}

dfsample=dataInput

dfsample['Input'].replace(str2num, regex=True, inplace=True)
dfsample['FD']=dfsample['Input'].str.extract('(\d)')
dfsample['LD']=dfsample['Input'].str.extract('(\d)(?=\D*$)')
dfsample['Int']=(dfsample['FD']+dfsample['LD']).astype(int)

dfsample['Int'].sum()
